<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from FacadeService WSDL service -->

  <service name="Facade" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://DefaultNamespace"/>
      <parameter name="wsdlServiceElement" value="FacadeService"/>
      <parameter name="schemaQualified" value="http://DefaultNamespace"/>
      <parameter name="wsdlServicePort" value="Facade"/>
      <parameter name="className" value="DefaultNamespace.FacadeSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Facade"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="main" qname="operNS:main" xmlns:operNS="http://DefaultNamespace" soapAction="" >
        <parameter qname="pns:args" xmlns:pns="http://DefaultNamespace" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="main"/>

  </service>
</deployment>
